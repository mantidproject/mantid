<ServerManagerConfiguration>
  <!-- Begin PeaksReader -->
  <ProxyGroup name="sources">
    <SourceProxy name="PeaksReader" class="vtkPeaksReader">
	    <StringVectorProperty
         name="WorkspaceTypeName"
         command="GetWorkspaceTypeName"
         number_of_elements="1"
         information_only="1">
        <SimpleStringInformationHelper /> 
       </StringVectorProperty>
       <StringVectorProperty
        name="FileName"
        command="SetFileName"
        number_of_elements="1">
        <FileListDomain name="files"/>
      </StringVectorProperty>
      
      <IntVectorProperty
                        name="Dimensions"
                        command="SetDimensions"
                        number_of_elements="1"
                        default_values="1">
        <EnumerationDomain name="enum">
          <Entry value="1" text="Q (Lab frame)"/>
          <Entry value="2" text="Q (Sample frame)"/>
          <Entry value="3" text="HKL"/>
        </EnumerationDomain>
        <Documentation>
          Choose the dimension space to plot the single-crystal peaks. This should match your loaded data file.
        </Documentation>
         </IntVectorProperty>
         <DoubleVectorProperty
           name="Unintegrated Peak Marker Size"
           command="SetUnintPeakMarkerSize"
           number_of_elements="1"
           default_values="0.3">
           <Documentation>
             Set the size of the unintegrated peak markers.
           </Documentation>
         </DoubleVectorProperty>
         <Hints>
           <ReaderFactory
             extensions="peaks"
             file_description="ISAW Peaks File" />
         </Hints>
    </SourceProxy>
  </ProxyGroup>
  <!-- End PeaksReader -->
</ServerManagerConfiguration>
