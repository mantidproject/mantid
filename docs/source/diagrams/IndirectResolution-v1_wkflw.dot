digraph IndirectResolution {
  label="IndirectResolution Flowchart"
  $global_style

  subgraph decision {
    $decision_style
    ScaleFactorIsOne [label="ScaleFactor is 1?"]
  }

  subgraph params {
    $param_style
    InputFiles
    Instrument
    Analyser
    Reflection
    DetectorRange
    BackgroundRange
    RebinParam
    ScaleFactor
    OutputWorkspace
  }

  subgraph algorithms {
    $algorithm_style
	ISISIndirectEnergyTransferWrapper
    ISISIndirectEnergyTransfer
    Scale
    CalculateFlatBackground
    Rebin
  }

  InputFiles                  -> ISISIndirectEnergyTransferWrapper
  Instrument                  -> ISISIndirectEnergyTransferWrapper
  Analyser                    -> ISISIndirectEnergyTransferWrapper
  Reflection                  -> ISISIndirectEnergyTransferWrapper
  DetectorRange               -> ISISIndirectEnergyTransferWrapper
  ISISIndirectEnergyTransferWrapper -> ISISIndirectEnergyTransfer
  ISISIndirectEnergyTransfer  -> ScaleFactorIsOne
  ScaleFactorIsOne            -> Scale [label="No"]
  ScaleFactor                 -> Scale
  ScaleFactorIsOne            -> CalculateFlatBackground [label="Yes"]
  Scale                       -> CalculateFlatBackground
  ISISIndirectEnergyTransfer  -> CalculateFlatBackground
  BackgroundRange             -> CalculateFlatBackground
  CalculateFlatBackground     -> Rebin
  RebinParam                  -> Rebin
  Rebin                       -> OutputWorkspace
}
