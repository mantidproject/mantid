  S_Vana_SaveFocusedXYE2        -> S_Vana_SaveFocusedXYE5
  S_Vana_SaveFocusedXYE3        -> S_Vana_SaveFocusedXYE5
  S_Vana_SaveFocusedXYE4        -> S_Vana_SaveFocusedXYE5

  }

