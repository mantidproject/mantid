  C_Vana_Divide                 -> C_Vana_NoAbs
  C_Vana_NoAbs                  -> C_Vana_RunCorrectAbs                 [label="true"]
  C_Vana_NoAbs                  -> C_Vana_DiffractionFocusing           [label="else"]
